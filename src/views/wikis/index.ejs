<!DOCTYPE html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <main>
      <% include ../static/partials/navbar.ejs %>

      <h1>Wikis</h1>
      <a href="/wikis/new" class="btn">New Wiki</a>
      <ul class="list-group">
        <% wikis.forEach((wiki) => { %>
          <li class="list-group-item">
            <a href="/wikis/<%= wiki.id %>"> <%= wiki.name %> </a>
          </li>
        <% }) %>
      </ul>

    </main>
  </body>
</html>